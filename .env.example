# District shapefile path (polygon features)
DISTRICT=/path/to/district.shp

# Building shapefile path (polygon features with 'Floor' attribute)
BUILDING=/path/to/buildings.shp

# Output directory for segmentation results
OUTPUT_DIR=/path/to/output

# Segmentation parameters
N_SEGMENTS=100
COMPACTNESS=10.0
SIMILARITY_THRESHOLD=0.5
